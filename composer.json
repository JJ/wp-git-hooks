{
    "name": "claudiosanches/wp-git-hooks",
    "description": "WP GIT Hooks",
    "type": "scripts",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Claudio Sanches",
            "email": "contato@claudiosanches.com"
        }
    ],
    "support": {
        "issues": "https://github.com/claudiosanches/wp-git-hooks/issues",
        "source": "https://github.com/claudiosanches/wp-git-hooks"
    },
    "keywords": [
        "WordPress"
    ],
    "require": {
        "php": ">=7.0",
        "wp-coding-standards/wpcs": "*",
        "wimg/php-compatibility": "^8.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.3"
    },
    "autoload": {
        "psr-4": {
            "ClaudioSanches\\WpGitHooks\\": "src/"
        }
    },
    "scripts": {
        "uninstall-wp-git-hooks": [
            "WooCommerce\\GitHooks\\Hooks::preHooks"
        ],
        "install-wp-git-hooks": [
            "WooCommerce\\GitHooks\\Hooks::postHooks"
        ]
    }
}